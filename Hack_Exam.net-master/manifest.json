{
  "name": "Exam Hack Program",
  "description": "This extension lets the user go in and out of exam.net without getting caught.",
  "version": "2.0",
  "manifest_version": 2,
    "permissions": [
    "webRequest",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "storage",
    "tabs",
    "activeTab",
    "browsingData"
  ],
  "background": {"scripts":["background.js"]},
  "browser_action": {
        "default_title": "Exam Hack Program",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/image16.png",
            "32": "images/image32.png",
            "48": "images/image48.png",
            "128": "images/image128.png"
        }
    },
    "icons": {
        "16": "images/image16.png",
        "32": "images/image32.png",
        "48": "images/image48.png",
        "128": "images/image128.png"
    },
  "web_accessible_resources": [
    "inject_scripts/inject_exitd.txt"
  ]
}